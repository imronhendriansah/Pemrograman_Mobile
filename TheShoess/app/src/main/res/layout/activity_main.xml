<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="932dp"
        android:layout_height="918dp"
        app:srcCompat="@drawable/desain_tanpa_judul"
        tools:layout_editor_absoluteX="-261dp"
        tools:layout_editor_absoluteY="-48dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="333dp"
        android:layout_height="154dp"
        app:srcCompat="@drawable/nama"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="376dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/the_shoess"
        tools:layout_editor_absoluteX="133dp"
        tools:layout_editor_absoluteY="37dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="297dp"
        android:layout_height="436dp"
        app:srcCompat="@drawable/masuk"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="37dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="272dp"
        android:layout_height="96dp"
        app:srcCompat="@drawable/buat_akun"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="594dp" />

    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="339dp"
        android:layout_height="547dp"
        android:background="#00FF5722"
        app:srcCompat="@drawable/eye"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="210dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="207dp"
        android:layout_height="47dp"
        android:text="MASUK"
        app:strokeColor="#FF5000"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="538dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="402dp"
        android:text="Masukan E-mail"/>

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="460dp"
        android:text="Masukan Password"/>

</androidx.constraintlayout.widget.ConstraintLayout>